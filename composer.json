{
    "name": "chestnut/dependency-injection",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Pogodda",
            "email": "daniel.pogodda@googlemail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chestnut\\DependencyInjection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chestnut\\DependencyInjection\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DanielLarusso/chestnut-utils"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/container": "^2.0",
        "chestnut/utils": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.14"
    }
}
